{
    "policyName": {
        "type": "String",
        "metadata": {
            "displayName": "policy name"
        },
        "defaultValue": "setByPolicy"
    },
    "storageAccountId": {
        "type": "String",
        "metadata": {
            "displayName": "storage account ID",
            "description": "ID of Landing Zone's storage account"
        }
    },
    "effect": {
        "type": "String",
        "metadata": {
            "displayName": "Effect",
            "description": "Enable or Disable the execution of the policy"
        },
        "allowedValues":[
            "AuditIfNotExists",
            "DeployIfNotExists",
            "Disabled"
        ],
        "defaultValue": "DeployIfNotExists"
    }
}
